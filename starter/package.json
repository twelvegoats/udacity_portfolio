{
    "name": "portfolio-homepage",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "watch": "npm-watch",
        "build-sass": "sass src/scss/main.scss:src/dist/main.css",
        "test": "echo "Error: no test specified" && exit 1"
    },
    "dependencies": {
